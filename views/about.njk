{% extends "layout.njk" %}


    {% block content %}
  <link rel="stylesheet" href="about.css">

</head>

<body>
  <div id="logo">
    <h1>{{about.title}}</h1>
    <h2>{{about.description}}</h2>
    <img src="{{about.image}}" alt="{{about.title}}">
    <ul>
      {{about.courses}}
    </ul>
  </div>
  <div class="links">
      
    <div class="social">
         {% for link in about.links %}
         <img src="{{link.image}}">
        <a href="{{link.url}}" target="_blank">{{link.name}}</a>
         {% endfor %}
    </div>
       
  </div>  
      
    {% endblock %}

    